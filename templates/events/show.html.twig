{% extends 'base.html.twig' %} {% block title %}Events{% endblock %} {% block
body %}
<h1>Events</h1>

<table class="table">
  <tbody>
    <tr>
      <th>Id</th>
      <td>{{ event.id }}</td>
    </tr>
    <tr>
      <th>Name</th>
      <td>{{ event.name }}</td>
    </tr>
    <tr>
      <th>Date</th>
      <td>{{ event.date ? event.date|date('Y-m-d H:i:s') : '' }}</td>
    </tr>
    <tr>
      <th>Description</th>
      <td>{{ event.description }}</td>
    </tr>
    <tr>
      <th>Image</th>
      <td>{{ event.image }}</td>
    </tr>
    <tr>
      <th>Capacity</th>
      <td>{{ event.capacity }}</td>
    </tr>
    <tr>
      <th>Email</th>
      <td>{{ event.email }}</td>
    </tr>
    <tr>
      <th>PhoneNumber</th>
      <td>{{ event.phoneNumber }}</td>
    </tr>
    <tr>
      <th>StreetName</th>
      <td>{{ event.streetName }}</td>
    </tr>
    <tr>
      <th>StreetNumber</th>
      <td>{{ event.streetNumber }}</td>
    </tr>
    <tr>
      <th>ZIP</th>
      <td>{{ event.ZIP }}</td>
    </tr>
    <tr>
      <th>City</th>
      <td>{{ event.city }}</td>
    </tr>
    <tr>
      <th>URL</th>
      <td>{{ event.URL }}</td>
    </tr>
    <tr>
      <th>EventType</th>
      <td>{{ event.eventType }}</td>
    </tr>
  </tbody>
</table>

<a class="btn btn-warning" href="{{ path('app_events_index') }}"
  >back to list</a
>

<a
  class="btn btn-primary"
  href="{{ path('app_events_edit', { id: event.id }) }}"
  >edit</a
>

{{ include("events/_delete_form.html.twig") }}
{% endblock %}
